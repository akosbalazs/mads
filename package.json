{
    "name": "mads_64tass",
    "displayName": "MADS/64tass",
    "description": "6502/65816 Assembly language, MADS/64tass syntax highlighting",
    "version": "0.0.7",
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#2d5e91",
        "theme": "dark"
    },
    "publisher": "akosbalazs",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/akosbalazs/mads/issues"
    },
    "homepage": "https://github.com/akosbalazs/mads/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/akosbalazs/mads.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "mads",
        "6502",
        "65816",
        "atari",
        "commodore",
        "64tass"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mads_64tass",
                "aliases": [
                    "6502/65816 Assembly"
                ],
                "extensions": [
                    ".asm",
                    ".mac",
                    ".hea",
                    ".src",
                    ".s"
                ],
                "configuration": "./mads.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mads_64tass",
                "scopeName": "source.asm.6502_65816.mads_64tass",
                "path": "./syntaxes/mads.tmLanguage"
            }
        ]
    }
}